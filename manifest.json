{
  "manifest_version": 3,
  "name": "Harpagon",
  "description": "To boldly read",
  "version": "3.3",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "declarativeNetRequest"
  ],

  "host_permissions": [
    "<all_urls>"    
  ]
  
}
